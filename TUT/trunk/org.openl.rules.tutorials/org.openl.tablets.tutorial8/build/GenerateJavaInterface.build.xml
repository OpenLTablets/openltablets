<project name="GenJavaWrapper" default="generate" basedir="../">
	<taskdef name="openlgen" classname="org.openl.conf.ant.JavaInterfaceAntTask"/>

<target name="generate">
	<echo message="Generating org.openl.tablets.tutorial8 Interface"/>

	<openlgen openlName="org.openl.xls" userHome="." 
		srcFile="rules/Tutorial_8.xls"
		targetClass="org.openl.tablets.tutorial8.Tutorial_8RulesInterface"
		displayName="Tutorial 8 - TBasic example"
		targetSrcDir="gen"		
	/>
</target>
</project>