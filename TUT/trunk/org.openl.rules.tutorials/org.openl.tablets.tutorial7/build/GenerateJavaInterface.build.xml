<project name="GenJavaWrapper" default="generate" basedir="../">
	<taskdef name="openlgen" classname="org.openl.conf.ant.JavaWrapperAntTask"/>

<target name="generate">
	<echo message="Generating org.openl.tablets.tutorial7 Interface"/>

	<openlgen openlName="org.openl.xls" userHome="." 
		srcFile="rules/Tutorial_7.xls"
		targetClass="org.openl.tablets.tutorial7.Tutorial_7RulesInterface"
		displayName="Tutorial 7 - ColumnMatch example"
		targetSrcDir="gen"
		goal="all with interface"
	/>
</target>
</project>