<project name="GenJavaWrapper" default="generate" basedir="../">

	<taskdef name="openlgen" classname="org.openl.conf.ant.JavaWrapperAntTask"/>
	
	
<target name="generate">
	<echo message="Generating org.openl.tablets.tutorial3 Interface"/>

	<openlgen openlName="org.openl.xls" userHome="." 
		srcFile="rules/Tutorial_3.xls"
	    targetClass="org.openl.tablets.tutorial3.Tutorial_3RulesInterface"
		displayName="Tutorial 3 - Advanced Decision and Data Tables"
		targetSrcDir="gen"
		goal="all with interface"
	>
	</openlgen>	
	
	<openlgen openlName="org.openl.xls" userHome="." 
		srcFile="rules/Tutorial_3_Tests.xls"
	    targetClass="org.openl.tablets.tutorial3.Tutorial_3TestRulesInterface"
		displayName="Tutorial 3 - With Tests"
		targetSrcDir="gen"
		goal="all with interface"
	>
	</openlgen>	
	
	
</target>
</project>