<project name="GenJavaWrapper" default="generate" basedir="../">
	<taskdef name="openlgen" classname="org.openl.conf.ant.JavaWrapperAntTask"/>

<target name="generate">
	<echo message="Generating org.openl.tablets.tutorial10 Interface"/>

	<openlgen openlName="org.openl.xls" userHome="." 
		srcFile="rules/Tutorial_10.xlsx"
		targetClass="org.openl.tablets.tutorial10.Tutorial_10RulesInterface"
		displayName="Tutorial 10 - Lookup table example - Special form of Rules table"
		targetSrcDir="gen"
		goal="all with interface"
	/>
</target>
</project>