<project name="GenJavaWrapper" default="generate" basedir="../">

	<taskdef name="openlgen" classname="org.openl.conf.ant.JavaWrapperAntTask"/>
	
	
<target name="generate">
	<echo message="Generating org.openl.tablets.tutorial6 Wrapper"/>

	<openlgen openlName="org.openl.xls" userHome="." 
		srcFile="rules/SudokuRules3.xls"
	    targetClass="org.openl.tablets.tutorial6.sudoku.SudokuWrapper"
		displayName="Sudoku Solver"
		
		targetSrcDir="gen"
	>
	</openlgen>	
	
	
	
</target>
</project>