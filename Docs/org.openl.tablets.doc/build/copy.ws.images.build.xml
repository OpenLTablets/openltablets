<project default="copy_images" basedir="..">

<property name="src_root" value="../org.openl.rules.webstudio/webapps/webstudio"/>
<property name="dst_root" value="site/testsite/images"/>
	
	
<target name = "copy_images">
	<copy preservelastmodified="true" 
			todir="${dst_root}">
	<fileset dir="${src_root}/images">
		<exclude name="**/*.db"/>
	</fileset> 
		
	</copy>
</target>	
	
	
	
	
</project>