<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <groupId>org.openl.rules</groupId>
    <artifactId>openl-bean-mapper-demo</artifactId>
    <packaging>jar</packaging>
    <version>1.0.0</version>
    <name>OpenL - Bean Mapper Demo</name>

    <dependencies>

        <dependency>
            <groupId>org.openl.rules</groupId>
            <artifactId>openl-bean-mapper</artifactId>
            <version>1.0.0</version>
        </dependency>

        <dependency>
            <groupId>com.exigen.ipb.crm</groupId>
            <artifactId>ipb-crm</artifactId>
            <version>2.7.3-HF5</version>
            <exclusions>
                <exclusion>
                    <groupId>org.springframework</groupId>
                    <artifactId>spring-faces</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>com.exigen.ipb.product.components</groupId>
                    <artifactId>ipb-components-ui</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>net.sf.jsfcomp</groupId>
                    <artifactId>clientvalidators</artifactId>
                </exclusion>

                <exclusion>
                    <groupId>com.exigen.ipb</groupId>
                    <artifactId>ipb-base-ui</artifactId>
                </exclusion>
            </exclusions>
        </dependency>

        <dependency>
            <groupId>com.exigen.chartis.products.chome</groupId>
            <artifactId>chome-policy-components</artifactId>
            <version>1.0.0-SNAPSHOT</version>
            <exclusions>
                <exclusion>
                    <groupId>org.springframework</groupId>
                    <artifactId>spring-faces</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>com.exigen.ipb.product.components</groupId>
                    <artifactId>ipb-components-ui</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>net.sf.jsfcomp</groupId>
                    <artifactId>clientvalidators</artifactId>
                </exclusion>

                <exclusion>
                    <groupId>com.exigen.ipb</groupId>
                    <artifactId>ipb-base-ui</artifactId>
                </exclusion>
            </exclusions>
        </dependency>

        <dependency>
            <groupId>com.exigen.chartis.domain</groupId>
            <artifactId>chartis-rating-domain</artifactId>
            <version>1.0.0-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>com.exigen.chartis.products.chome</groupId>
            <artifactId>chome-policy-domain</artifactId>
            <version>1.0.0-SNAPSHOT</version>
        </dependency>


        <dependency>
            <groupId>com.exigen.chartis.integration</groupId>
            <artifactId>chartis-integration-acord-beans</artifactId>
            <version>1.0.0-SNAPSHOT</version>
        </dependency>

    </dependencies>

    <build>
        <plugins>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>

        </plugins>

    </build>

</project>