{
  "openapi": "3.0.1",
  "info": {
    "title": "Reusable requests twice",
    "version": "1.0.0"
  },
  "paths": {
    "/test": {
      "post": {
        "summary": "test",
        "description": "test",
        "requestBody": {
          "$ref": "#/components/requestBodies/TestRequest"
        },
        "responses": {
          "200": {
            "content": {
              "text/plain": {
                "schema": {
                  "type": "number",
                  "format": "double"
                }
              }
            },
            "description": "test response"
          }
        }
      }
    },
    "/oneMoreTest": {
      "post": {
        "summary": "test",
        "description": "test",
        "requestBody": {
          "$ref": "#/components/requestBodies/TestRequest"
        },
        "responses": {
          "200": {
            "content": {
              "text/plain": {
                "schema": {
                  "type": "number",
                  "format": "double"
                }
              }
            },
            "description": "test response"
          }
        }
      }
    }
  },
  "components": {
    "requestBodies": {
      "TestRequest": {
        "description": "test request body",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/RequestModel"
            }
          }
        }
      }
    },
    "schemas": {
      "MyModel": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "USER_ERROR",
              "RULES_RUNTIME",
              "COMPILATION",
              "SYSTEM",
              "BAD_REQUEST",
              "VALIDATION"
            ]
          },
          "details": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "RequestModel": {
        "type": "object",
        "properties": {
          "count": {
            "type": "integer"
          }
        }
      }
    }
  }
}