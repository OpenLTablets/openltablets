{
  "openapi": "3.0.1",
  "info": {
    "title": "Reusable responses",
    "version": "1.0.0",
    "description": "reusable response project example; result: "
  },
  "paths": {
    "/test": {
      "post": {
        "summary": "test",
        "description": "test",
        "requestBody": {
          "$ref": "#/components/requestBodies/TestRequest"
        },
        "responses": {
          "200": {
            "$ref": "#/components/responses/TestResponse"
          }
        }
      }
    }
  },
  "components": {
    "requestBodies": {
      "TestRequest": {
        "description": "test request body",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/RequestModel"
            }
          }
        }
      }
    },
    "responses": {
      "TestResponse": {
        "description": "test response",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/ResponseModel"
            }
          }
        }
      }
    },
    "schemas": {
      "MyModel": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "USER_ERROR",
              "RULES_RUNTIME",
              "COMPILATION",
              "SYSTEM",
              "BAD_REQUEST",
              "VALIDATION"
            ]
          },
          "details": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "RequestModel": {
        "type": "object",
        "properties": {
          "count": {
            "type": "number"
          }
        }
      },
      "ResponseModel": {
        "type": "object",
        "properties": {
          "height": {
            "type": "number"
          }
        }
      }
    }
  }
}