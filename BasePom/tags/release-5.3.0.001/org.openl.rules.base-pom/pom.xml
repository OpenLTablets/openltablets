  <distributionManagement>
    <repository>
      <id>exigen-repo-release</id>
      <name>Exigen Release Repository</name>
      <url>file:////bldrix02.exigengroup.lv/maven-repo/exigen-repo-release</url>
    </repository>
    <snapshotRepository>
      <id>exigen-repo-snapshot</id>
