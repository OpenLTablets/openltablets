<project>
	<id>multi-module-overloaded-static</id>
	<name>Multi module overloading test</name>

	<!-- OpenL project includes one or more modules.  -->
	<modules>

		<module>
			<name>MODULE</name>
			<type>api</type>
			<!-- 
				Rules root document. Usually excel file on file system. 
			-->
			<rules-root path="project*/Module*.xlsx"/>

		</module>	
	</modules>

	<classpath>
		<entry path="."/>
		<entry path="bin"/>
		<entry path="target/classes"/>
		<entry path="lib"/>
	</classpath>	

</project>