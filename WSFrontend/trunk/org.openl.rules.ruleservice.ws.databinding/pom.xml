<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>org.openl.rules</groupId>
		<artifactId>org.openl.rules.ruleservice-pom</artifactId>
		<version>5.10.2</version>
		<relativePath>../org.openl.rules.ruleservice-pom</relativePath>
	</parent>

	<groupId>org.openl.rules</groupId>
	<artifactId>org.openl.rules.ruleservice.ws.databinding</artifactId>
	<packaging>jar</packaging>
	<name>OpenL - RuleService - RuleService - Web Services Databinding</name>
	<description>OpenL Rules Webservice Databinding</description>
	<url>http://openl-tablets.sf.net/</url>

	<build>
		<resources>
			<resource>
				<directory>src</directory>
				<includes>
					<include>**/*.properties</include>
				</includes>
			</resource>
			<resource>
				<directory>conf</directory>
				<includes>
					<include>**/*.properties</include>
					<include>**/*.xml</include>
				</includes>
			</resource>
			<resource>
				<directory>binding-conf/aegis</directory>
			</resource>
			<resource>
				<directory>binding-conf/jaxb</directory>
			</resource>
		</resources>
	</build>
	<dependencies>
		<dependency>
			<groupId>org.apache.cxf</groupId>
			<artifactId>cxf-rt-databinding-aegis</artifactId>
		</dependency>
		<dependency>
			<groupId>commons-logging</groupId>
			<artifactId>commons-logging</artifactId>
		</dependency>
		<dependency>
			<groupId>org.openl.rules</groupId>
			<artifactId>org.openl.rules</artifactId>
			<optional>true</optional>
		</dependency>
		<dependency>
			<groupId>org.openl.rules</groupId>
			<artifactId>org.openl.rules.project</artifactId>
			<optional>true</optional>
		</dependency>
	</dependencies>
</project>
