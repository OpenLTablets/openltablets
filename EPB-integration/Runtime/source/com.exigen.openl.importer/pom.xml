<?xml version="1.0" encoding="UTF-8"?>
<project>
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.exigen.openl</groupId>
	<artifactId>importer</artifactId>
	<parent>
		<groupId>com.exigen.openl</groupId>
		<artifactId>openl</artifactId>
		<version>7.0.0-SNAPSHOT</version>
		<relativePath>../com.exigen.openl</relativePath>
	</parent>
	<name>Exigen OpenL Rules Importer (JAR file)</name>
	<description>
		This project contains OpenL Rules importer that reads rules from
		Excel file and save them into OpenL Rules EMF model.
	</description>
	<dependencies>
		<dependency>
			<groupId>com.exigen.openl</groupId>
			<artifactId>model</artifactId>
			<version>${pom.version}</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>build-helper-maven-plugin</artifactId>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
			</plugin>
		</plugins>
	</build>
</project>
