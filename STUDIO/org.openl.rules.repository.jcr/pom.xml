		<artifactId>org.openl.rules.studio-pom</artifactId>
		<version>5.13.3</version>
	</parent>

	<artifactId>org.openl.rules.repository.jcr</artifactId>
	<packaging>jar</packaging>
	<name>OpenL - STUDIO - Repository Jcr</name>
	<description>JSR 170 based OpenL Repository implementation</description>
	<url>http://openl-tablets.sf.net/</url>

	<build>
		<testResources>
			<testResource>
				<directory>test</directory>
				<includes>
					<include>**/*.properties</include>
					<include>**/*.xml</include>
				</includes>
			</testResource>
		</testResources>

		<resources>
			<resource>
				<directory>src</directory>
				<includes>
					<include>**/*.properties</include>
					<include>**/*.xml</include>
				</includes>
			</resource>
		</resources>
	</build>

