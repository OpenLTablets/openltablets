<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

  <application>
    <variable-resolver>org.springframework.web.jsf.DelegatingVariableResolver</variable-resolver>

    <locale-config>
      <default-locale>en</default-locale>
      <supported-locale>en</supported-locale>
    </locale-config>
  </application>

  <managed-bean>
    <managed-bean-name>skinBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.jsf.SkinBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>

  <component>
    <component-type>org.openl.commons.web.jsf.facelets.acegi.Authentication</component-type>
    <component-class>org.openl.commons.web.jsf.facelets.acegi.AuthenticationComponent</component-class>
  </component>

  <managed-bean>
    <managed-bean-name>mainBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.MainBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>openTableBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.OpenTableBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>systemSettingsBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.admin.SystemSettingsBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>treeViewBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.TreeViewBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <!-- repository config -->
  <managed-bean>
    <managed-bean-name>rulesUserSession</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.servlet.RulesUserSession</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
      <property-name>workspaceManager</property-name>
      <value>#{workspaceManager}</value>
    </managed-property>
    <managed-property>
      <property-name>user</property-name>
      <value>#{currentUserInfo.user}</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>deploymentController</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.repository.DeploymentController</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
      <property-name>repositoryTreeState</property-name>
      <value>#{repositoryTreeState}</value>
    </managed-property>
    <managed-property>
      <property-name>deploymentManager</property-name>
      <value>#{deploymentManager}</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>repositoryTreeState</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.repository.RepositoryTreeState</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
      <property-name>userWorkspace</property-name>
      <value>#{rulesUserSession.userWorkspace}</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>repositoryArtefactPropsHolder</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.repository.RepositoryArtefactPropsHolder</managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>repositoryTreeController</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.repository.RepositoryTreeController</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
      <property-name>userWorkspace</property-name>
      <value>#{rulesUserSession.userWorkspace}</value>
    </managed-property>
    <managed-property>
      <property-name>zipFilter</property-name>
      <value>#{zipFilter}</value>
    </managed-property>
    <managed-property>
      <property-name>repositoryTreeState</property-name>
      <value>#{repositoryTreeState}</value>
    </managed-property>
    <managed-property>
      <property-name>repositoryArtefactPropsHolder</property-name>
      <value>#{repositoryArtefactPropsHolder}</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>dependencyController</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.repository.DependencyController</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
      <property-name>repositoryTreeState</property-name>
      <value>#{repositoryTreeState}</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>smartRedeployController</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.repository.SmartRedeployController</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
      <property-name>repositoryTreeState</property-name>
      <value>#{repositoryTreeState}</value>
    </managed-property>
    <managed-property>
      <property-name>deploymentManager</property-name>
      <value>#{deploymentManager}</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>repositoryDiffController</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.repository.diff.RepositoryDiffController</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
      <property-name>repositoryTreeState</property-name>
      <value>#{repositoryTreeState}</value>
    </managed-property>
    <managed-property>
      <property-name>designTimeRepository</property-name>
      <value>#{designTimeRepository}</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>uploadExcelDiffController</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.diff.UploadExcelDiffController</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>

  <!-- studio config -->
  <managed-bean>
    <managed-bean-name>benchmarkMethodBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.ui.BenchmarkMethodBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>studioOpener</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.studio.StudioFromLWSController</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
      <property-name>localWorkspaceManager</property-name>
      <value>#{localWorkspaceManager}</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>localUpload</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.repository.LocalUploadController</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>advancedSearchRequest</managed-bean-name>
    <managed-bean-class>org.openl.rules.ui.search.OpenLAdvancedSearchBean$SearchRequest</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>

    <managed-property>
      <property-name>advancedSearchBean</property-name>
      <value>#{advancedSearch}</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>advancedSearch</managed-bean-name>
    <managed-bean-class>org.openl.rules.ui.search.OpenLAdvancedSearchBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>simpleSearchBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.search.SimpleSearchBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>indexSearchBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.search.IndexSearchBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>searchResultsBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.search.SearchResultsBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>showTableBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.tableeditor.ShowTableBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>showTraceTableBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.trace.ShowTraceTableBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>showExplainTableBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.explain.ShowExplainTableBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>explainBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.explain.ExplainBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>rulesDependenciesBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.RulesDependenciesBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>traceIntoFileBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.trace.TraceIntoFileBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>tableViewMenuBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.TableViewMenuBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>showMessageBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.tableeditor.ShowMessageBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>messagesBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.MessagesBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>runAllTestsBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.test.RunAllTestsBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>runTestMethodBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.test.RunTestMethodBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>headerBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.HeaderBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>treeBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.TreeBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>problemsBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.ProblemsBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>revertProjectChangesBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.RevertProjectChangesBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>traceTreeBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.TraceTreeBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>explainTreeBean</managed-bean-name>
    <managed-bean-class>org.openl.rules.webstudio.web.ExplainTreeBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>bussinessSearch</managed-bean-name>
    <managed-bean-class>org.openl.rules.ui.search.BussinesSearchPropertyBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  
  <managed-bean>
    <managed-bean-name>bussinessSearchRequest</managed-bean-name>
    <managed-bean-class>org.openl.rules.ui.search.BussinesSearchPropertyBean$BussinessSearchRequest</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>

    <managed-property>
      <property-name>bussinessSearchBean</property-name>
      <value>#{bussinessSearch}</value>
    </managed-property>
  </managed-bean>

  <navigation-rule>
    <from-view-id>/facelets/repository/main.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>webstudio</from-outcome>
      <to-view-id>/index.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>

  <!-- wizard config -->
  <managed-bean>
    <managed-bean-name>OpenLTypes</managed-bean-name>
    <managed-bean-class>org.openl.rules.ui.tablewizard.OpenLTypes</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
    <managed-property>
      <property-name>options</property-name>
      <list-entries>
        <value>int</value>
        <value>String</value>
        <value>Date</value>
      </list-entries>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>counter</managed-bean-name>
    <managed-bean-class>org.openl.rules.ui.tablewizard.jsf.Counter</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  
  <managed-bean>
    <managed-bean-name>tableCreatorWizardManager</managed-bean-name>
    <managed-bean-class>org.openl.rules.ui.tablewizard.TableWizardManager</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>tableCopierWizardManager</managed-bean-name>
    <managed-bean-class>org.openl.rules.ui.tablewizard.TableCopierWizardManager</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>domainTreePath</managed-bean-name>
    <managed-bean-class>org.openl.rules.ui.tablewizard.DomainTreePath</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>

    <managed-property>
      <property-name>domainTree</property-name>
      <value>#{tableCreatorWizardManager.wizard.domainTree}</value>
    </managed-property>
    <managed-property>
      <property-name>parameters</property-name>
      <value>#{tableCreatorWizardManager.wizard.parameters}</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>tableCopier</managed-bean-name>
    <managed-bean-class>org.openl.rules.ui.copy.TableNamesCopier</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>

  <navigation-rule>
    <navigation-case>
      <from-outcome>mainPage</from-outcome>
      <to-view-id>/openTable.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
      <navigation-case>
      <from-outcome>wizardSelect</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable0.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>newTableCancel</from-outcome>
      <to-view-id>/home.html</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/facelets/tableWizard/newTable0.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>newTable</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable1.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>newDatatypeTable</from-outcome>
      <to-view-id>/facelets/tableWizard/datatype/newDatatypeTable1.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>newDatatypeAliasTable</from-outcome>
      <to-view-id>/facelets/tableWizard/datatypeAlias/newDatatypeAliasTable1.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>newTestTable</from-outcome>
      <to-view-id>/facelets/tableWizard/newTableTest1.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>newTableProperty</from-outcome>
      <to-view-id>/facelets/tableWizard/newTableProperty1.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <!-- Decision table wizard -->
  <navigation-rule>
    <from-view-id>/facelets/tableWizard/newTable1.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>next</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable2.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>prev</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable0.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/facelets/tableWizard/newTable2.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>next</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable3.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>prev</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable1.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/facelets/tableWizard/newTable3.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>next</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable4.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>prev</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable2.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/facelets/tableWizard/newTable4.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>next</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable5.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>prev</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable3.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/facelets/tableWizard/newTable5.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>next</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable6.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>prev</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable4.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/facelets/tableWizard/newTable6.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>prev</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable5.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <!-- Datatype table wizard -->
  <navigation-rule>
    <from-view-id>/facelets/tableWizard/datatype/newDatatypeTable1.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>next</from-outcome>
      <to-view-id>/facelets/tableWizard/datatype/newDatatypeTable2.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>prev</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable0.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  
  <navigation-rule>
    <from-view-id>/facelets/tableWizard/datatype/newDatatypeTable2.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>next</from-outcome>
      <to-view-id>/facelets/tableWizard/datatype/newDatatypeTable3.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>prev</from-outcome>
      <to-view-id>/facelets/tableWizard/datatype/newDatatypeTable1.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/facelets/tableWizard/datatype/newDatatypeTable3.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>next</from-outcome>
      <to-view-id>/facelets/tableWizard/datatype/newDatatypeTable4.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>prev</from-outcome>
      <to-view-id>/facelets/tableWizard/datatype/newDatatypeTable2.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/facelets/tableWizard/datatype/newDatatypeTable4.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>prev</from-outcome>
      <to-view-id>/facelets/tableWizard/datatype/newDatatypeTable3.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>

  <!-- Datatype Alias table wizard -->
  <navigation-rule>
    <from-view-id>/facelets/tableWizard/datatypeAlias/newDatatypeAliasTable1.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>next</from-outcome>
      <to-view-id>/facelets/tableWizard/datatypeAlias/newDatatypeAliasTable2.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>prev</from-outcome>
      <to-view-id>/facelets/tableWizard/newTable0.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  
  <navigation-rule>
    <from-view-id>/facelets/tableWizard/datatypeAlias/newDatatypeAliasTable2.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>next</from-outcome>
      <to-view-id>/facelets/tableWizard/datatypeAlias/newDatatypeAliasTable3.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>prev</from-outcome>
      <to-view-id>/facelets/tableWizard/datatypeAlias/newDatatypeAliasTable1.xhtml</to-view-id>
    </navigation-case>
