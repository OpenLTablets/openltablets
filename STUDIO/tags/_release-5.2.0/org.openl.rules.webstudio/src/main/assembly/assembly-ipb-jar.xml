<?xml version="1.0" encoding="UTF-8"?>
<assembly>
  <id>ipb-ui</id>
  <includeBaseDirectory>false</includeBaseDirectory>

  <formats>
    <format>jar</format>
  </formats>

  <fileSets>
    <fileSet>
      <directory>webapps/webstudio</directory>
      <outputDirectory></outputDirectory>
      <includes>
        <include>**/*</include>
      </includes>
    </fileSet>

    <fileSet>
      <directory>webapps/webstudio/WEB-INF/faces</directory>
      <outputDirectory>META-INF</outputDirectory>
      <includes>
        <include>faces-config.xml</include>
      </includes>
    </fileSet>
  </fileSets>

</assembly>
