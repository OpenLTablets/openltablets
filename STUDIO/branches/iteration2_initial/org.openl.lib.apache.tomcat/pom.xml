<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>org.openl.rules</groupId>
    <artifactId>org.openl.rules.studio-pom</artifactId>
    <version>5.0.5</version>
    <relativePath>../org.openl.rules.studio-pom/pom.xml</relativePath>
  </parent>

  <groupId>org.openl</groupId>
  <artifactId>org.openl.lib.apache.tomcat</artifactId>
  <name>org.openl.lib.apache.tomcat</name>
  <description></description>

  <repositories>
    <repository>
      <id>local</id>
      <url>file://${basedir}/../${parent.artifactId}/m2_repo</url>
    </repository>
  </repositories>

  <build>
    <defaultGoal>package</defaultGoal>

    <resources>
      <resource>
        <directory>apache-tomcat-5.5.17</directory>
        <targetPath>apache-tomcat-5.5.17</targetPath>
      </resource>
    </resources>

    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-eclipse-plugin</artifactId>
        <configuration>
          <skip>true</skip>
        </configuration>
      </plugin>

      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jar-plugin</artifactId>
        <configuration>
          <archive>
            <manifestFile>META-INF/MANIFEST.MF</manifestFile>
          </archive>
        </configuration>
      </plugin>

    </plugins>
  </build>

  <dependencies>
  </dependencies>

</project>
