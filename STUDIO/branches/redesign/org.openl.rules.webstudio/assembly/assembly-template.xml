<?xml version="1.0" encoding="UTF-8"?>
<assembly>
  <id>template</id>
  <includeBaseDirectory>false</includeBaseDirectory>

  <formats>
    <format>zip</format>
  </formats>

  <fileSets>
    <fileSet>
      <directory>.</directory>
      <outputDirectory></outputDirectory>
      <includes>
        <include>*.*</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>web</directory>
      <outputDirectory>webapps/webstudio</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>META-INF</directory>
    </fileSet>
	<fileSet>
        <directory>target/classes</directory>
        <outputDirectory>webapps/webstudio/WEB-INF/classes</outputDirectory>
    </fileSet>
  </fileSets>

</assembly>
