{
  "openapi": "3.0.1",
  "info": {
    "title": "OpenL Tablets WebStudio API",
    "version": "*"
  },
  "servers": [
    {
      "url": "http://localhost:##"
    }
  ],
  "paths": {
    "/files/form/{name}": {
      "post": {
        "operationId": "form",
        "parameters": [
          {
            "name": "name",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "file"
                ],
                "type": "object",
                "properties": {
                  "configuration": {
                    "type": "string",
                    "description": "Pam-Pam"
                  },
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "default": {
            "description": "default response"
          }
        }
      }
    },
    "/files/upload": {
      "post": {
        "operationId": "upload",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "file"
                ],
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "default": {
            "description": "default response"
          }
        }
      }
    },
    "/files/upload/bulk": {
      "post": {
        "operationId": "bulkUpload",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "files"
                ],
                "type": "object",
                "properties": {
                  "files": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "default": {
            "description": "default response"
          }
        }
      }
    }
  },
  "components": {}
}