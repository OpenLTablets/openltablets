<?xml version="1.0"?>

<project name="build" default="copy.webstudio.project" basedir="..">

<property name="src_root" value="../../Studio/org.openl.rules.webstudio"/>
<property name="dst_root" value="templates.templates/org.openl.rules.webstudio"/>

<target name = "copy.webstudio.project">
	
	<copy preservelastmodified="true" todir="${dst_root}">
		<fileset dir="${src_root}">
			<exclude name="**/CVS/**" />
			<exclude name="**/.svn/**" />
			<exclude name="**/bin/**" />
			<exclude name="**/work/**" />
		</fileset>
	</copy>
	
</target>	

</project>