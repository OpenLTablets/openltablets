/*
 * Expression syntax follows.
 */

void Expression() :
{
	Token x;
	String func = null;
}
{
	
	
  ConditionalTernaryExpression()
  [


      ( x = "=" {func="op.assign";}
      | x = "+=" {func="op.assign.add";} 
      | x = "-=" {func="op.assign.subtract";}
      | x = "*=" {func="op.assign.multiply";} 
      | x = "/=" {func="op.assign.divide";}
      | x = "&=" {func="op.assign.bitand";}
      | x = "|=" {func="op.assign.bitor";}
      | x = "^=" {func="op.assign.bitnot";}
      | x = "%=" {func="op.assign.rem";} 
    )
    Expression()
    {
      syntaxBuilder.bop(func, pos(x));
    }
  ]
}


void ConditionalTernaryExpression() :
{Token x;}
{
  ConditionalImplExpression()
  [ x="?" ConditionalTernaryExpression() ":" ConditionalTernaryExpression()
    { syntaxBuilder.nop("op.ternary.qmark", pos(x), 3); }
  ]
}

void ConditionalImplExpression() :
{Token x;}
{
  ConditionalOrExpression()
  (
    LOOKAHEAD( <IMPL> )
    (x = <IMPL>) ConditionalOrExpression() 
    { syntaxBuilder.bop("op.binary.impl", pos(x)); }
  )*
}

void ConditionalOrExpression() :
{Token x;}
{
  ConditionalAndExpression() ( (x=<BOOL_OR> | x = "or") ConditionalAndExpression() 
  { syntaxBuilder.bop("op.binary.or", pos(x)); } )*
}


void ConditionalAndExpression() :
{Token x;}
{
  BitwiseOrExpression() 
  ( (x=<BOOL_AND>| x = "and") BitwiseOrExpression()  
  { syntaxBuilder.bop("op.binary.and", pos(x)); } )*
}



void BitwiseOrExpression() :
{Token x;}
{
  BitwiseXorExpression() (LOOKAHEAD(2) (x=<BIT_OR>) BitwiseXorExpression() 
  { syntaxBuilder.bop("op.binary.bitor", pos(x)); } )*
}

void BitwiseXorExpression() :
{Token x;}
{
  BitwiseAndExpression() ( 
  x=<BIT_XOR> BitwiseAndExpression() 
  { syntaxBuilder.bop("op.binary.bitxor", pos(x)); } )*
}

void BitwiseAndExpression() :
{Token x;}
{
  EqualityExpression() 
  ( (x=<BIT_AND>) EqualityExpression()  
  { syntaxBuilder.bop("op.binary.bitand", pos(x)); } )*
}



void EqualityExpression() :
{
 Token x;
 String func = null;
}
{
  RelationalExpression()
  (
    (
      (x = <EQ>
      	| x = "equals to"
      	| x = "is same as") { func="eq"; }
      |(x = <NE>
      	| x = "does not equal to"
      	| x = "is different from") { func="ne"; }
      |(x = <STRICT_EQ>) { func="strict_eq"; }
   	  |(x = <STRICT_NE>) { func="strict_ne"; }
	)
    RelationalExpression()
    {syntaxBuilder.bop("op.binary." + func, pos(x));}
  )*
}

void OfTheExpression() :
{
 Token x;
}
{
  Expression()
  (
    ( x = <OF>    <THE>)
    Expression()
    {syntaxBuilder.bop("of.the" , pos(x));}
  )
}


void RelationalExpression() :
{
	Token x;
	String func = null;
}
{
  ShiftExpression()
  (
    (
      x = <LT> { func="lt";}
      | x = "is less than" { func="lt";}
      | x = <GT> { func="gt";}
      | x = "is more than" { func="gt";}
      | x = <LE> { func="le";}
      | x = "is less or equal" { func="le";}
      | x = "is no more than" { func="le";}
      | x = "is in" { func="le";}
      | x = <GE> { func="ge";}
      | x = "is more or equal" { func="ge";}
      | x = "is no less than" { func="ge";}
      | x = <STRICT_GT> { func="strict_gt";}
      | x = <STRICT_GE> { func="strict_ge";}
      | x = <STRICT_LT> { func="strict_lt";}
      | x = <STRICT_LE> { func="strict_le";}
    )
    ShiftExpression()
    {
    	{syntaxBuilder.bop("op.binary." + func, pos(x));}
    }
  )*
}


void ShiftExpression() :
{
Token x;
String func = null;
}
{
  AdditiveExpression()
  (
    (x=<LSHIFT> {func="lshift";}| x=<RSIGNEDSHIFT> {func="rshift";} | x=<RUNSIGNEDSHIFT> {func="rshiftu";})
    AdditiveExpression()
    {syntaxBuilder.bop("op.binary." + func, pos(x));}
  )*
}


void AdditiveExpression() :
{
Token x;
String func = null;
}
{
  MultiplicativeExpression()
  (
    (x=<PLUS> {func="add";}| x=<PLUSSTR> {func="add";} | x=<MINUS> {func="subtract";})
    MultiplicativeExpression()
    {syntaxBuilder.bop("op.binary." + func, pos(x));}
  )*
}

void MultiplicativeExpression() :
{
Token x;
String func = null;
}
{
  PowerExpression()
  (
    ( x = <STAR>{func="multiply";} | x = <SLASH> {func="divide";}| x = <REM> {func="rem";})
    PowerExpression()
    {syntaxBuilder.bop("op.binary." + func, pos(x));}
  )*
}

void PowerExpression() :
{Token x;}
{
  UnaryExpression()
  (
    LOOKAHEAD( <EXP> )
    x=<EXP> UnaryExpression()
    {syntaxBuilder.bop("op.binary.pow", pos(x));}
  )*
}

void UnaryExpression() :
{Token x;}
{
	

  x=<PLUS> UnaryExpression()     
  {syntaxBuilder.uop("op.unary.positive", pos(x));}

|
  x=<MINUS> UnaryExpression() 
  {syntaxBuilder.uop("op.unary.negative", pos(x));}
|
  x=<TILDE> UnaryExpression() 
  {syntaxBuilder.uop("op.unary.bitnot", pos(x));}
|
  x=<BANG> UnaryExpression() 
  {syntaxBuilder.uop("op.unary.not", pos(x));}
|
  PreIncrementExpression()
|
  PreDecrementExpression()
|
  LOOKAHEAD( CastLookahead() )
  CastExpression()
|
  PostfixExpression()
}

void PreIncrementExpression() :
{Token x;}
{
  x="++" PrimaryExpression() 
  { syntaxBuilder.uop("op.prefix.inc", pos(x)); }
}

void OfTheExpressionSuffix() :
{Token x;}
{
  x=<OF> <THE> PrimaryExpression() {syntaxBuilder.bop("chain.suffix.of.the",null);}
}


void PreDecrementExpression() :
{Token x;}
{
  x="--" PrimaryExpression() { syntaxBuilder.uop("op.prefix.dec", pos(x)); }
}

void PostfixExpression() :
{Token x;}
{
  PrimaryExpression() 
  [ x="++"  
	  { syntaxBuilder.uop("op.suffix.inc", pos(x)); }
   | x="--"  
	  { syntaxBuilder.uop("op.suffix.dec", pos(x)); }
  ]
}

