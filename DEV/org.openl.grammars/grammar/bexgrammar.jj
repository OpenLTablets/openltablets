	{syntaxBuilder.nop("list", null, elements);}
}

void function() :
{
  String funcname;
  int args = 0;
  Token x1, x2;
}
{
  x1=<IDENTIFIER> {funcname = token.image;}
  "("
  [ VariableInitializer() {args++;} ( "," VariableInitializer() {args++;})* ]
  x2=")"
  {
  	syntaxBuilder.identifier("funcname", pos(x1), funcname);
    syntaxBuilder.nop("function", pos(x1,x2), args + 1);
  }

}

void AllocationExpression() :
{
  int args = 0;
  Token x1, x2;
}
{
  x1="new" ClassName()
  (
    "("
    [ Expression() {args++;} ( "," Expression() {args++;})* ]
    x2=")"
    {
      syntaxBuilder.nop("op.new.object",pos(x1, x2), args + 1);
    }

    |

    ArrayDimsAndInits()
    {
      syntaxBuilder.uop("op.new.array",pos(x1));
    }
  )

}

/*
 * The second LOOKAHEAD specification below is to parse to PrimarySuffix
 * if there is an expression between the "[...]".
 */
void ArrayDimsAndInits() :
{
	Token x1, x2;
	int cnt = 0; 
}
{
  LOOKAHEAD(2)
  ( LOOKAHEAD(2) x1="[" Expression() x2="]" {syntaxBuilder.bop("array.index.expression", pos(x1, x2)); cnt++;} )+
  ( LOOKAHEAD(2) x1="[" x2="]" {syntaxBuilder.uop("array.index.empty", pos(x1, x2));} )*
|
  ( TypeDimension() )+ ArrayInitializer()
  {syntaxBuilder.bop("new.array.initialized", null);}
}


void IndexTypeExpression() :
{Token x1,x2;}
{
 LOOKAHEAD(3)
  (x1 = "(" Type() VariableDeclaratorId() x2 = ")" 
  
    {
      	syntaxBuilder.nop("index.parameter.declaration", pos(x1, x2), 2);
  	}
  )
|
 LOOKAHEAD(3)
  (x1 = "(" VariableDeclaratorId() x2 = ")" 
  
    {
      	syntaxBuilder.nop("index.parameter.declaration", pos(x1, x2), 1);
  	}
  )

  
  }

void IndexOperator() :
{Token x1,x2, x3; String prefix;}
{
   LOOKAHEAD(6)
  (x1="[" IndexTypeExpression()  (
      (x3="@ " | x3="select all having" | x3="select all where " ) {prefix="selectall";}  | 
      (x3="!@" | x3="select first having" | x3="select first where") {prefix="selectfirst";}  |
      (x3="^@" | x3="order by" | x3="order increasing by") {prefix="orderby";}  |
      (x3="v@" | x3="order decreasing by") {prefix="orderdecreasingby";}  |
      (x3="~@" | x3="s@" | x3="split by") {prefix="splitby";}  |
      (x3="*@" | x3="transform to") {prefix="transform";}  |
      (x3="*!@" | x3="transform unique to") {prefix="transformunique";}  
      )  
      Expression()
  x2="]"
  {
    syntaxBuilder.nop(prefix +".index", pos(x1, x2), 2);
    
  })
|
LOOKAHEAD(2)
  ( x1="["
    Expression()
  x2="]"
  {
    syntaxBuilder.uop("op.index", pos(x1, x2));
    
  })
|
  ( (x1="[!@" | x1="[select first having " | x1="[select first where ") Expression()
  x2="]"
  {
    syntaxBuilder.uop("selectfirst.index", pos(x1, x2));
    
  })
|
  ( (x1="[@" | x1="[select all having " | x1="[select all where ") Expression()
  x2="]"
  {
    syntaxBuilder.uop("selectall.index", pos(x1, x2));
    
  })
|
  ( (x1="[^@" | x1="[order by " | x1="[order increasing by ") Expression()
  x2="]"
  {
    syntaxBuilder.uop("orderby.index", pos(x1, x2));
    
  })
|
  ( (x1="[v@" | x1="[order decreasing by ") Expression()
  x2="]"
  {
    syntaxBuilder.uop("orderdecreasingby.index", pos(x1, x2));
    
  })
|
  ( (x1="[~@" | x1="[s@" | x1="[split by ") Expression()
  x2="]"
  {
    syntaxBuilder.uop("splitby.index", pos(x1, x2));
    
  })
|
  ( (x1="[*@" | x1="[transform to ") Expression()
  x2="]"
  {
    syntaxBuilder.uop("transform.index", pos(x1, x2));
    
  })
|
  ( (x1="[*!@" | x1="[transform unique to ") Expression()
  x2="]"
  {
    syntaxBuilder.uop("transformunique.index", pos(x1, x2));
    
  })
  
  
  
  
}

