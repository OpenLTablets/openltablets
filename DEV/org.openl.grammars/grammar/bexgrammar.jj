void PrimarySuffix() :
{Token x1;}
{
  IndexOperator() {syntaxBuilder.bop("chain.suffix.dot.index", null);}
  | LOOKAHEAD( "." function() ) "." function() {syntaxBuilder.bop("chain.suffix.dot.function", null);}
 // | "of the" PrimaryExpression()
  | OfTheExpressionSuffix()
  | "." identifier() {syntaxBuilder.bop("chain.suffix.dot.identifier", null);}
  
//  | x1 = <IDENTIFIER> {syntaxBuilder.identifier("identifier.chain", pos(token),token.image);}
}

