void CommaSeparatedList() :
{
	int elements = 0;
	int words = 0;
}
{
	"[" 
	{words = 0;} 
	( 
		 
	    Word("identifier.list") {++words;})+     
		{syntaxBuilder.nop("list.element", null, words); ++elements;}
	 
	("," {words =0;}
