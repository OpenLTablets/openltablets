{
  int args = 1;
}
{
	
  PrimaryPrefix() ( LOOKAHEAD(2) PrimarySuffix() {++args;} )* 
  {
  	if (args > 1)
  		syntaxBuilder.uop("chain", null);
  }
  
  
}





