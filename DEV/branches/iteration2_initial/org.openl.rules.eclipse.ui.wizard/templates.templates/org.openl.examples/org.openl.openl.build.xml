<project default="build.org.openl.openl">

<taskdef name="openl" classname="org.openl.conf.ant.AntOpenLTask"/>


 <property file="org.openl.examples.properties"/>


<target name="build.org.openl.openl">
<!--	
   <echo message="Building org.openl.openl ..."/>	
--> 
 	<ant antfile="org.openl.dom.by.example.build.xml" 
    target="build.org.openl.dom.by.example" 
    dir="../lang.config"
 	/>	
 
   <openl category="org.openl.openl" 
     extendscategory="org.openl.dom.by.example"
     description="This is openl extension of a org.openl.j.xml.dom">
 
  <types>
    <typelibrary namespace="org.openl.this">

      <schema factory="dom.factory" file="src/xml/Person.xml" />
      <schema factory="dom.factory" file="src/xml/People.xml" />
      <schema factory="dom.factory" file="src/xml/MyCustomer.xml" />

      <javaimport all="${openl.local.imports}"/>
   
    </typelibrary>
  </types>
    
 </openl>
 	
</target>



</project>
