<project default="build.org.openl.j.science">

<taskdef name="openl" classname="org.openl.conf.ant.AntOpenLTask"/>
 
	 <property file="../lang.config/OpenL.properties"/>
	

<target name="build.org.openl.j.science">
<!--	
   <echo message="Building org.openl.j.science ... "/>	
-->
 
	<ant antfile="org.openl.j.build.xml" target="build.org.openl.j"/>

 
   <openl category="org.openl.j.science" classpath="${org.openl.j.science.cp}" shared="true"
     extendscategory="org.openl.j"  
     description="This is an extension of a org.openl.j">
 
  <libraries>
    <library namespace="org.openl.this">
      <javalib classname="java.lang.Math"/>
      <javalib classname="org.openl.types.science.Operators"/>
    </library>

    <library namespace="org.openl.operators">
      <javalib classname="org.openl.types.science.Operators"/>
    </library>

  </libraries>
 </openl>
</target>

</project>