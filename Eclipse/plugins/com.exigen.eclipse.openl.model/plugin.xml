<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="com.exigen.eclipse.common.facet.emf.model">
      <model-descriptor
            descriptor-class="com.exigen.eclipse.openl.model.OpenLModelType"
            file-extension="openl"
            id="com.exigen.eclipse.openl.model"/>
      <model-descriptor
            descriptor-class="com.exigen.eclipse.openl.model.ExcelFileModelType"
            file-extension="xls"
            id="com.exigen.eclipse.openl.excelFile"/>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.generated_package">
      <package
            class="com.exigen.openl.model.openl.OpenlPackage"
            genModel="models/openl.genmodel"
            uri="http://exigengroup.com/openl"/>
   </extension>

</plugin>
